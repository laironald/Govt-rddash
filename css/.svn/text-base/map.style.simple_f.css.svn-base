	background: none;
	color: #f7f7f7;
	padding: 2px;
	border: 1px solid #666666;
	width: 200px;
}
.contact input.place, .contact input.subject, .contact textarea, select {
	border: 1px solid #111;
	font-size: 10pt;
	background: #333333;
	color: #f7f7f7;
}
#agency, #polyselect {
	width: 210px;
}
.contact input.place, .contact input.subject, .contact textarea {
	padding: 5px;
	margin-top: 10px;
	margin-bottom: 10px;
	width: 400px;
}
.contact textarea { 
	width: 550px;
	height: 200px; 
}

.qtip {
	font-size: 8pt;
}
